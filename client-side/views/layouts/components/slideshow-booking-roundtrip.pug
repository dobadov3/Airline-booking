                img(src='/images/slideshow/plane-2.jpg', alt='', data-uk-cover='')
                | &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;    &#x9;
                .uk-overlay-primary.uk-position-cover.impx-overlay.dark
              | &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;    
            // Slideshow Item #2 End
            // Slideshow Item #3
            li
              .uk-position-cover.uk-animation-kenburns.uk-animation-reverse.uk-transform-origin-center-left
                img(src='/images/slideshow/plane-3.jpg', alt='', data-uk-cover='')
                | &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;    &#x9;
