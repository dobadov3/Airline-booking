              | &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;    
            // Slideshow Item #3 End
            // Slideshow Item #4
            li
              .uk-position-cover.uk-animation-kenburns.uk-animation-reverse.uk-transform-origin-bottom-right
                img(src='/images/slideshow/plane-4.jpg', alt='', data-uk-cover='')
                | &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;    &#x9;
